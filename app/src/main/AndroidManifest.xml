<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ekisan">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ima"
        android:label="app-name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.PostDetails"></activity>
        <activity
            android:name=".Activities.Home1"
            android:label="@string/title_activity_home1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.Home" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Home1Activity" />
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.RegisterActivity" />
    </application>

</manifest>